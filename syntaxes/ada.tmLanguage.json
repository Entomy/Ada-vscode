									"end": ";",
									"captures": {
										"0": {
											"patterns": [
												{ "include": "#punctuation" }
											]
										}
									},
									"patterns": [
										{ "include": "#attribute" },
										{ "include": "#value" },
										{ "include": "#type" },
										{ "include": "#punctuation" },
										{ "include": "#keyword" }
