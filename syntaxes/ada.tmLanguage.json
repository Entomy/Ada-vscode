{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Ada",
	"foldingStartMarker": "\\bbegin\\b",
	"foldingStopMarker": "\\bend\\b",
	"patterns": [
		{ "include": "#aspect" },
		{ "include": "#assignment" },
		{ "include": "#attribute" },
		{ "include": "#comment" },
		{ "include": "#goto" },
		{ "include": "#label" },
		{ "include": "#pragma" },
		{ "include": "#punctuation" },
		{ "include": "#subroutine" },
		{ "include": "#type" },
		{ "include": "#keyword" },
		{ "include": "#value" }
	],
	"repository": {
		"aggregate": {
			"patterns": [
				{
					"name": "meta.aggregate.ada",
					"begin": "\\(",
					"end": "\\)",
					"captures": {
						"0": {
							"patterns": [
								{ "include": "#punctuation" }
							]
						}
					},
					"patterns": [
						{ "include": "#value" },
						{ "include": "#punctuation" }
					]
				}	
			]
		},
		"arguments": {
			"name": "meta.arguments.ada",
			"begin": "\\(",
			"end": "\\)",
			"captures": {
				"0": {
					"patterns": [
						{ "include": "#punctuation" }
					]
				}
			},
			"patterns": [
				{ "include": "#argument" },
				{ "include": "#punctuation" }
			]
		},
		"argument": {
			"patterns": [
				{
					"name": "meta.argument.ada",
					"match": "(\\w|_)+\\s*=>\\s*(\\w|_)+",
					"captures": {
						"0": {
							"patterns": [
								{
									"name": "meta.positional-name.ada",
									"match": "\\b(\\w|_)+\\s*=>",
									"captures": {
										"0": {
											"patterns": [
												{ "include": "#punctuation" },
												{
													"name": "entity.name.selector.ada",
													"match": "\\b(\\w|_)+\\b"
												}
											]
										}
									}
								},
								{ "include": "#value" }
							]
						}
					}
				},
				{
					"name": "meta.argument.ada",
					"match": "(\\w|_)+\\s*=>\\s*\".*\"",
					"captures": {
						"0": {
							"patterns": [
								{
									"name": "meta.positional-name.ada",
									"match": "\\b(\\w|_)+\\s*=>",
									"captures": {
										"0": {
											"patterns": [
												{ "include": "#punctuation" },
												{
													"name": "entity.name.selector.ada",
													"match": "\\b(\\w|_)+\\b"
												}
											]
										}
									}
								},
								{ "include": "#value" }
							]
						}
					}
				},
				{
					"name": "meta.argument.ada",
					"match": "\\b((\\w|_)+\\s*,\\s*)*(\\w|_)+\\s*:\\s*(in\\s+)?(out\\s+)?(\\w|\\.|_)+(\\s*:=\\s*(\\w|\\.|_|')+)?",
					"captures": {
						"0": {
							"patterns": [
								{
									"name": "meta.type.annotation.ada",
									"match": ":\\s*(in\\s+)?(out\\s+)?(\\w|\\.|_)+(\\s*:=\\s*(\\w|\\.|_|')+)?",
									"captures": {
										"0": {
											"patterns": [
												{
													"name": "meta.type.annotation.default.ada",
													"match": ":=\\s*(\\w|\\.|_|')+\\b",
													"captures": {
														"0": {
															"patterns": [
																{ "include": "#punctuation" },
																{ "include": "#attribute" },
																{ "include": "#value" }
															]
														}
													}
												},
												{ "include": "#keyword" },
												{ "include": "#punctuation" },
												{
													"name": "storage.type.ada",
													"match": "\\b(\\w|\\.|_)+"
												}
											]
										}
									}
								},
								{
									"name": "variable.parameter.ada",
									"match": "\\b((\\w|_)+\\s*,\\s*)*(\\w|_)+\\b",
									"captures": {
										"0": {
											"patterns": [
												{ "include": "#punctuation" }
											]
										}
									}
								}
							]
						}
					}
				},
				{
					"name": "meta.argument.ada",
					"match": "\\b(\\w|\\.|_)+\\b",
					"captures": {
						"0": {
							"patterns": [
								{ "include": "#value" }
							]
						}
					}
				},
				{
					"name": "meta.argument.ada",
					"match": "'.'",
					"captures": {
						"0": {
							"patterns": [
								{ "include": "#value" }
							]
						}
					}
				},
				{
					"name": "meta.argument.ada",
					"match": "\".*\"",
					"captures": {
						"0": {
							"patterns": [
								{ "include": "#value" }
							]
						}
					}
				}
			]
		},
		"aspect": {
			"patterns": [
				{
					"name": "meta.aspect.ada",
					"begin": "\\bwith\\b",
					"end": "(\\bis\\b|;)",
					"captures": {
						"0": {
							"patterns": [
								{ "include": "#keyword" }
							]
						}
					},
					"patterns": [
