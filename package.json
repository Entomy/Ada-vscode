{
    "name": "ada",
    "displayName": "Ada",
    "description": "Ada language support, developed by an actual Ada programmer",
    "icon": "icon.png",
    "version": "0.88.0",
    "publisher": "Entomy",
    "author": "Entomy",
    "galleryBanner": {
        "color": "#AAAAAA",
        "theme": "light"
    },
    "preview": true,
    "keywords": [
        "ada",
        "adb",
        "ads",
        "gpr",
        "ali"
    ],
    "license": "LICENSE",
    "repository": {
        "type": "git",
        "url": "https://github.com/Entomy/Ada-vscode"
    },
    "bugs": {
        "url": "https://github.com/Entomy/Ada-vscode/issues",
        "email": "p-kell@live.com"
    },
    "engines": {
        "vscode": "^1.19.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "grammars": [
            {
                "language": "ada",
                "scopeName": "source.ada",
                "path": "./grammars/ada.grammar.json"
            }
        ],
        "languages": [
            {
                "id": "ada",
                "aliases": [ "ada", "Ada", "ADA", "spark", "Spark", "SPARK" ],
                "extensions": [ ".ada", ".adb", ".ads" ],
                "configuration": "./languages/ada.language.json"
            }
        ]
    }
}
