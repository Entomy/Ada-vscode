{
    "name": "ada",
    "displayName": "Ada",
    "description": "Ada language support, developed by an actual Ada programmer",
    "icon": "icon.png",
    "version": "0.66.0",
    "publisher": "Entomy",
    "author": "Entomy",
    "galleryBanner": {
        "color": "#AAAAAA",
        "theme": "light"
    },
    "preview": true,
    "keywords": [
        "ada",
        "adb",
        "adc",
        "ads",
        "gpr",
        "ali"
    ],
    "license": "LICENSE",
    "repository": {
        "type": "git",
        "url": "https://github.com/Entomy/Ada-vscode"
    },
    "bugs": {
        "url": "https://github.com/Entomy/Ada-vscode/issues",
        "email": "p-kell@live.com"
    },
    "engines": {
        "vscode": "^1.19.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "grammars": [
            {
                "language": "ada",
                "scopeName": "source.ada",
                "path": "./syntaxes/ada.tmLanguage.json"
            },
            {
                "language": "adc",
                "scopeName": "source.adc",
                "path": "./syntaxes/adc.tmLanguage.json"
            },
            {
                "language": "ali",
                "scopeName": "source.ali",
                "path": "./syntaxes/ali.tmLanguage.json"
            },
            {
                "language": "gpr",
                "scopeName": "source.gpr",
                "path": "./syntaxes/gpr.tmLanguage.json"
            }
        ],
        "languages": [
            {
                "id": "ada",
                "aliases": [
                    "Ada",
                    "ADA",
                    "ada",
                    "Spark",
                    "SPARK",
                    "spark"
                ],
                "extensions": [
                    ".adb",
                    ".ads"
                ],
                "configuration": "./ada.language.json"
            },
            {
                "id": "adc",
                "aliases": [
                    "GNAT Configuration",
                    "GNAT Config",
                    "adc"
                ],
                "extensions": [
                    ".adc"
                ],
                "configuration": "./adc.language.json"
            },
            {
                "id": "ali",
                "aliases": [
                    "ali",
                    "ALI",
                    "Ada Library Information"
                ],
                "extensions": [
                    ".ali"
                ]
            },
            {
                "id": "gpr",
                "aliases": [
                    "GNAT Project",
                    "gpr"
                ],
                "extensions": [
                    ".gpr"
                ],
                "configuration": "./gpr.language.json"
            }
        ]
    }
}
